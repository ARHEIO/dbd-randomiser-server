{
  "name": "typescript-lambda-template",
  "version": "1.1.2",
  "description": "A base package for lambdas that compiles to javascript",
  "main": "index.js",
  "scripts": {
    "start": "npm run docker:build && npm run docker:start",
    "build": "rm -rf dist ; webpack --config webpack.config.js",
    "docker:build": "rm -rf dist ; webpack --config ./docker/webpack-docker.config.js",
    "docker:start": "docker-compose up --build",
    "test": "jest --watchAll=true --config=jest.config.js",
    "test:ci": "npm test -- --silent --watchAll=false",
    "test:e2e": "docker-compose -f docker-compose.yml -f ./test/docker-compose.test.yml up --build",
    "coverage": "npm test -- --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@amindunited/to-camel-case": "0.0.0",
    "@types/aws-lambda": "^8.10.52",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.37",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "aws-lambda-router": "^0.8.3",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "jest": "^26.0.1",
    "mockserver-client": "^5.10.0",
    "path": "^0.12.7",
    "serverless": "^1.60.5",
    "serverless-domain-manager": "^4.1.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "terser-webpack-plugin": "^2.3.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^3.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/jest": "^25.2.3",
    "dynamoose": "^2.2.0",
    "lodash": "^4.17.15"
  }
}
